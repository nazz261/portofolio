{
  "version": 2,
  "builds": [
    {
      "src": "api/index.php",
      "use": "@vercel/php"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/build/(.*)",
      "dest": "/build/$1"
    },
    {
      "src": "/(.*\\.(css|js|ico|png|jpg|jpeg|gif|svg|woff|woff2|ttf|eot))",
      "dest": "/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ],
  "env": {
    "APP_NAME": "Portfolio",
    "APP_ENV": "production",
    "APP_DEBUG": "false",
    "APP_URL": "https://portofolio-ddqanz79u-nazz261s-projects.vercel.app",
    "DB_CONNECTION": "sqlite",
    "CACHE_DRIVER": "array",
    "SESSION_DRIVER": "array",
    "LOG_LEVEL": "error"
  }
}